{
  "name": "precise-ui",
  "version": "0.5.0",
  "description": "Precise UI React component library.",
  "main": "dist/es6",
  "typings": "dist/es6",
  "scripts": {
    "start": "styleguidist server",
    "serve": "pushstate-server styleguide 6060",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --project tsconfig.json",
    "test:unit": "cross-env NODE_ENV=test jest src",
    "test:watch": "npm run test:unit -- --watch",
    "test": "npm run lint && npm run test:unit",
    "prettier": "prettier --config prettier.config.js --write 'src/**/*.{ts,tsx}'",
    "build:bundle": "cross-env NODE_ENV=production webpack -p",
    "build:es5": "build-with-transform-jsx -t es5 --outDir dist/es5 --declaration --inlineSourceMap --inlineSources --config ./tsconfig.json",
    "build:es6": "build-with-transform-jsx -t es6 -m es6 --outDir dist/es6 --declaration --inlineSourceMap --inlineSources --config ./tsconfig.json",
    "build:demo": "cross-env NODE_ENV=production styleguidist build",
    "build": "rimraf dist && npm run build:bundle && npm run build:es5 && npm run build:es6",
    "icongen": "node tools/convert-icons.js",
    "prepush": "npm run test & npm run build",
    "test:visual": "docker run --shm-size 1G -it -u `id -u $USER` --rm -v $(pwd):/usr/src/app --cap-add=SYS_ADMIN zenika/alpine-chrome:with-puppeteer node tools/visual-test.js",
    "test:visual-update": "node tools/visual-test.js -- -u"
  },
  "keywords": [
    "react",
    "spa",
    "app",
    "component",
    "library",
    "zeiss",
    "ux"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "ZEISS Digital Innovation Partners",
  "license": "MIT",
  "dependencies": {
    "memoize-one": "^5.0.0",
    "react-onclickoutside": "^6.7.1",
    "typescript-plugin-inner-jsx": "^0.1.9"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8",
    "styled-components": "^2.x"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.0",
    "@types/jest": "^23.3.12",
    "@types/memoize-one": "^4.1.0",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-onclickoutside": "^6.7.0",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "circular-dependency-plugin": "^5.0.2",
    "cross-env": "^5.0.5",
    "css-loader": "^1.0.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "jest-styled-components": "^6.3.1",
    "material-design-icons": "^3.0.1",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.15.3",
    "pushstate-server": "^3.0.1",
    "react": "~16.8.0",
    "react-docgen-typescript": "^1.12.3",
    "react-dom": "^16.8.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-styleguidist": "^8.0.6",
    "react-styleguidist-visual": "^0.8.0",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "styled-components": "^2.2.3",
    "svg-react-loader": "^0.4.6",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "3.2.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
